{
  "manifest_version": 3,
  "name": "Stream Translator",
  "version": "1.0",
  "key":"", 
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'",
    "sandbox": "sandbox allow-scripts allow-forms",
    "web_accessible_resources": [
      "home_popup.js"
    ]
  },
  "description": "Automated live stream/video call transcription and translation",
  "permissions": ["identity","activeTab", "storage"],
  "icons": {
    "48": "ST Logo.png"
  },
  "action": {
    "default_icon": "ST Logo.png",
    "default_popup": "signin_popup.html",
    "default_title": "Stream Translator"
  },
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}
